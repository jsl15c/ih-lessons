<h2>Sign Up</h2>
<p>Sign up to this awesome app 🐙</p>

<% if (typeof dumbUser ==='string') { %>
    <p class="dumbUser"><%= dumbUser %></p>
<%}%>

<form id="form" method="post" action="/signup">
  <label for="full-name-input"Your Name>Your Name</label>
  <input id="full-name-input" type="text" name="signupFullName" placeholder="Alan Turing"/>
  <br />

  <label for="username-input">Username</label>
  <input id="username-input" type="text" name="signupUsername" placeholder="turingator" />
  <br />

  <label for="password-input">Password</label>
  <input id="password-input" type="password" name="signupPassword" placeholder="********" />
  <br /><br />

  <button>Sign Up</button>
</form>
